<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD38AQZVRs2dxiv52Xju_pV2ntuIdV-d0g&libraries=places"></script>
    <script src="https://js.api.here.com/v3/3.1/mapsjs-core.js"></script>
    <script src="https://js.api.here.com/v3/3.1/mapsjs-service.js"></script>

    {% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body>
    <nav class="navbar navbar-expand-lg  bg-success">
        <div class="container">

            <a class="navbar-brand" href="">
                <img src="{{asset("images/logo.png")}}" width="200px" />

            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarColor01">
                <ul class="navbar-nav me-auto  " style="margin-left: 15%;">

                    <li class="nav-item ms-4">
                        <a href="{{path('index')}}" class="nav-link fs-5 ms-4  item-menu">Accueil</a>
                    </li>
                    <li class="nav-item">
                        <a href="#faq" class="nav-link fs-5 ms-4 item-menu">FAQ</a>
                    </li>
                    <li class="nav-item">
                        <a href="#contact" class="nav-link fs-5 ms-4 item-menu">Contact</a>
                    </li>
                </ul>
                <ul class="navbar-nav social-fluid">
                    {% if app.user %}
                    {% if app.user.roles[0] ==  "ROLE_ADMIN" %}
                    <li class="nav-item">
                        <a href="{{path('app_admin')}}"><button class="btn btn-nav ">Administration</button></a>
                    </li>
                    {% endif %}

                    <li class="nav-item ms-3">
                        <a href="{{path('app_logout')}}"><button class="btn btn-nav">Déconnexion</button></a>
                    </li>
